{
  "name": "google-actions-github-bot",
  "version": "0.0.1",
  "description": "A Google Assistant Action (with an invocation name of \"Github Trending\") which tells you the trending repositories on Github and more.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "watch": "tsc -w",
    "serve": "concurrently \"npm run watch\" \"firebase serve --only functions\"",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "actions-on-google": "^2.0.1",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "firebase-admin": "~5.12.0",
    "firebase-functions": "^1.0.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "concurrently": "^3.5.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "license": "MIT"
}
